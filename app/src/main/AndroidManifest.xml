<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="djiby.example.com.mmmtp4">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowCustomerInFoActivity"></activity>
        <activity android:name=".ListViewActivity"></activity>
        <provider
            android:name=".CustomerProvider"
            android:authorities="djiby.example.com.mmmtp4.CustomerProvider"
            android:label="Customer data" />
    </application>

</manifest>